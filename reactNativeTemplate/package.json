{
  "name": "MyTemplate",
  "version": "4.2.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "check-dependencies": "rnx-dep-check",
    "fix-dependencies": "rnx-dep-check --write"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-navigation/drawer": "6.5.3",
    "@react-navigation/native": "6.0.16",
    "@react-navigation/stack": "6.3.7",
    "@reduxjs/toolkit": "1.9.1",
    "@rneui/base": "4.0.0-rc.7",
    "@rneui/themed": "4.0.0-rc.7",
    "@tanstack/react-query": "4.18.0",
    "axios": "0.27.2",
    "axios-auth-refresh": "3.3.4",
    "formik": "2.2.9",
    "lodash": "4.17.21",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-bootsplash": "4.3.3",
    "react-native-config": "1.4.11",
    "react-native-gesture-handler": "2.8.0",
    "react-native-keyboard-aware-scroll-view": "0.9.5",
    "react-native-reanimated": "2.13.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "3.18.2",
    "react-native-toast-message": "2.1.5",
    "react-native-vector-icons": "9.2.0",
    "react-redux": "8.0.5",
    "twrnc": "3.4.1",
    "zod": "3.19.1",
    "zod-formik-adapter": "1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@babel/runtime": "7.12.5",
    "@react-native-community/eslint-config": "2.0.0",
    "@rnx-kit/dep-check": "1.12.20",
    "@tsconfig/react-native": "2.0.2",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.184",
    "@types/react": "18.0.21",
    "@types/react-native": "0.70.6",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "babel-jest": "26.6.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-native": "4.0.0",
    "husky": "7.0.4",
    "jest": "26.6.3",
    "lint-staged": "12.4.1",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "4.8.3"
  },
  "resolutions": {
    "@types/react": "17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint --ext .js,.jsx --fix-dry-run"
    ]
  },
  "rnx-kit": {
    "reactNativeVersion": "0.70.6",
    "kitType": "app",
    "capabilities": [
      "animation",
      "babel-preset-react-native",
      "core",
      "core-android",
      "core-ios",
      "gestures",
      "jest",
      "navigation/native",
      "navigation/stack",
      "react",
      "react-test-renderer",
      "safe-area",
      "screens",
      "storage"
    ]
  }
}
